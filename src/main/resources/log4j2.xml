<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
<Appenders>
    <Console name="Console">
        <PatternLayout pattern="%level{WARN=W, DEBUG=D, ERROR=E, TRACE=T, INFO=I} %d{HH:mm:ss.SSS} [%t] %logger{1} - %msg%n"/>
    </Console>
</Appenders>
<Loggers>
    <Logger name="com.crashbox" level="trace" additivity="true"/>
    <Root level="warn">
        <AppenderRef ref="Console"/>
    </Root>
</Loggers>
</Configuration>
